<%- include('header'); -%>
<body>

   <div class="box" id="heading">
    <h1>
      <%= listTitle %>
    </h1>
   </div>
 
  <div class="box">
    <% for(var i=0;i<listNewItems.length;i++){ %>
      <div class="item">
        <input type="checkbox">
      <p >
        <%= listNewItems[i] %>   
      </p>
      </div>
      <% }%>

      <form  action="/" method="post">
        <input placeholder="New item" name="newItem" type="text" autocomplete="off">
        <button type="submit" name="list" value="<%= listTitle %>">+</button>
      </form>
  </div>
    
</body>
<%- include('footer'); -%>

